     TOFILE(CORSOGIT/QCMDSRC)     + 
     FROMMBR(RELOAD)              + 
     TOMBR(*FROMMBR)              +
     MBROPT(*REPLACE)   
CRTBNDCL PGM(CORSOGIT/CONV_A2E)   +
         SRCFILE(CORSOGIT/QCLSRC) +
         SRCMBR(CONV_A2E)       
CRTMNU     MENU(CORSOGIT/CORSOGIT) TYPE(*UIM) SRCFILE(QTEMP/QMNUSRC)
DLTSPLF    FILE(CORSOGIT) SPLNBR(*LAST)
